{
  "version": "1.0",
  "description": "Validator registry for GPT-Trader validation framework",
  "validators": {
    "base": {
      "Validator": {
        "name": "Validator",
        "module": "gpt_trader.validation.base_validators",
        "docstring": "Base validator class.\n\nProvides optional predicate support so simple validation rules can be\nexpressed without creating a dedicated subclass. Subclasses can override\n:meth:`validate` to provide richer behaviour.",
        "parameters": {
          "error_message": {
            "required": false,
            "default": null,
            "type": "str | None"
          },
          "predicate": {
            "required": false,
            "default": null,
            "type": "collections.abc.Callable[[typing.Any], bool | tuple[bool, typing.Any]] | None"
          }
        },
        "validate_params": [
          "value",
          "field_name"
        ],
        "description": "Base validator class with optional predicate support",
        "usage": "Validator(predicate=lambda x: x > 0, error_message='Must be positive')"
      }
    },
    "type": {
      "TypeValidator": {
        "name": "TypeValidator",
        "module": "gpt_trader.validation.type_validators",
        "docstring": "Validate that value is of specific type",
        "parameters": {
          "expected_type": {
            "required": true,
            "type": "<class 'type'>"
          },
          "error_message": {
            "required": false,
            "default": null,
            "type": "str | None"
          }
        },
        "validate_params": [
          "value",
          "field_name"
        ],
        "description": "Validates value is of expected type",
        "usage": "TypeValidator(expected_type=int)"
      },
      "RangeValidator": {
        "name": "RangeValidator",
        "module": "gpt_trader.validation.type_validators",
        "docstring": "Validate that value is within range",
        "parameters": {
          "min_value": {
            "required": false,
            "default": null,
            "type": "float | None"
          },
          "max_value": {
            "required": false,
            "default": null,
            "type": "float | None"
          },
          "inclusive": {
            "required": false,
            "default": true,
            "type": "<class 'bool'>"
          },
          "error_message": {
            "required": false,
            "default": null,
            "type": "str | None"
          }
        },
        "validate_params": [
          "value",
          "field_name"
        ],
        "description": "Validates numeric value is within range",
        "usage": "RangeValidator(min_value=0, max_value=100)"
      },
      "ChoiceValidator": {
        "name": "ChoiceValidator",
        "module": "gpt_trader.validation.type_validators",
        "docstring": "Validate that value is one of allowed choices",
        "parameters": {
          "choices": {
            "required": true,
            "type": "list[typing.Any]"
          },
          "error_message": {
            "required": false,
            "default": null,
            "type": "str | None"
          }
        },
        "validate_params": [
          "value",
          "field_name"
        ],
        "description": "Validates value is one of allowed choices",
        "usage": "ChoiceValidator(choices=['BUY', 'SELL'])"
      },
      "RegexValidator": {
        "name": "RegexValidator",
        "module": "gpt_trader.validation.type_validators",
        "docstring": "Validate that string matches regex pattern",
        "parameters": {
          "pattern": {
            "required": true,
            "type": "<class 'str'>"
          },
          "error_message": {
            "required": false,
            "default": null,
            "type": "str | None"
          }
        },
        "validate_params": [
          "value",
          "field_name"
        ],
        "description": "Validates string matches regex pattern",
        "usage": "RegexValidator(pattern=r'^[A-Z]+-[A-Z]+$')"
      }
    },
    "numeric": {
      "PositiveNumberValidator": {
        "name": "PositiveNumberValidator",
        "module": "gpt_trader.validation.numeric_validators",
        "docstring": "Validate positive number",
        "parameters": {
          "allow_zero": {
            "required": false,
            "default": false,
            "type": "<class 'bool'>"
          },
          "error_message": {
            "required": false,
            "default": null,
            "type": "str | None"
          }
        },
        "validate_params": [
          "value",
          "field_name"
        ],
        "description": "Validates value is a positive number",
        "usage": "PositiveNumberValidator()"
      },
      "PercentageValidator": {
        "name": "PercentageValidator",
        "module": "gpt_trader.validation.numeric_validators",
        "docstring": "Validate percentage (0-100 or 0-1)",
        "parameters": {
          "as_decimal": {
            "required": false,
            "default": true,
            "type": "<class 'bool'>"
          },
          "error_message": {
            "required": false,
            "default": null,
            "type": "str | None"
          }
        },
        "validate_params": [
          "value",
          "field_name"
        ],
        "description": "Validates value is a valid percentage (0-1 or 0-100)",
        "usage": "PercentageValidator(as_decimal=True)"
      }
    },
    "pattern": {
      "SymbolValidator": {
        "name": "SymbolValidator",
        "module": "gpt_trader.validation.pattern_validators",
        "docstring": "Validate trading symbol (equities or crypto).\n\nAccepts common formats such as:\n- Equities: \"AAPL\", \"MSFT\"\n- Crypto spot: \"BTC-USD\", \"ETH-USD\"\n- Perpetuals: \"BTC-PERP\", \"ETH-PERP\"",
        "parameters": {
          "error_message": {
            "required": false,
            "default": null,
            "type": "str | None"
          },
          "predicate": {
            "required": false,
            "default": null,
            "type": "collections.abc.Callable[[typing.Any], bool | tuple[bool, typing.Any]] | None"
          }
        },
        "validate_params": [
          "value",
          "field_name"
        ],
        "description": "Validates trading symbol format (e.g., BTC-USD)",
        "usage": "SymbolValidator()"
      },
      "StrategyNameValidator": {
        "name": "StrategyNameValidator",
        "module": "gpt_trader.validation.pattern_validators",
        "docstring": "Validate strategy name identifier.\n\nAccepts names like 'SimpleMAStrategy', 'Momentum', 'mean_reversion'.\nMust start with a letter; allows letters, numbers, underscores and dashes.",
        "parameters": {
          "error_message": {
            "required": false,
            "default": null,
            "type": "str | None"
          },
          "predicate": {
            "required": false,
            "default": null,
            "type": "collections.abc.Callable[[typing.Any], bool | tuple[bool, typing.Any]] | None"
          }
        },
        "validate_params": [
          "value",
          "field_name"
        ],
        "description": "Validates strategy name format",
        "usage": "StrategyNameValidator()"
      }
    },
    "temporal": {
      "DateValidator": {
        "name": "DateValidator",
        "module": "gpt_trader.validation.temporal_validators",
        "docstring": "Validate date/datetime",
        "parameters": {
          "min_date": {
            "required": false,
            "default": null,
            "type": "datetime.date | datetime.datetime | None"
          },
          "max_date": {
            "required": false,
            "default": null,
            "type": "datetime.date | datetime.datetime | None"
          },
          "error_message": {
            "required": false,
            "default": null,
            "type": "str | None"
          }
        },
        "validate_params": [
          "value",
          "field_name"
        ],
        "description": "Validates and parses date strings",
        "usage": "DateValidator(format='%Y-%m-%d')"
      }
    },
    "data": {
      "DataFrameValidator": {
        "name": "DataFrameValidator",
        "module": "gpt_trader.validation.data_validators",
        "docstring": "Validate pandas DataFrame",
        "parameters": {
          "required_columns": {
            "required": false,
            "default": null,
            "type": "list[str] | None"
          },
          "min_rows": {
            "required": false,
            "default": null,
            "type": "int | None"
          },
          "error_message": {
            "required": false,
            "default": null,
            "type": "str | None"
          }
        },
        "validate_params": [
          "value",
          "field_name"
        ],
        "description": "Validates pandas DataFrame structure",
        "usage": "DataFrameValidator(required_columns=['open', 'high', 'low', 'close'])"
      },
      "SeriesValidator": {
        "name": "SeriesValidator",
        "module": "gpt_trader.validation.data_validators",
        "docstring": "Validate pandas Series",
        "parameters": {
          "error_message": {
            "required": false,
            "default": null,
            "type": "str | None"
          }
        },
        "validate_params": [
          "value",
          "field_name"
        ],
        "description": "Validates pandas Series",
        "usage": "SeriesValidator()"
      },
      "OHLCDataValidator": {
        "name": "OHLCDataValidator",
        "module": "gpt_trader.validation.data_validators",
        "docstring": "Validate OHLC price data",
        "parameters": {
          "error_message": {
            "required": false,
            "default": null,
            "type": "str | None"
          }
        },
        "validate_params": [
          "value",
          "field_name"
        ],
        "description": "Validates OHLC candlestick data integrity",
        "usage": "OHLCDataValidator()"
      }
    },
    "composite": {
      "CompositeValidator": {
        "name": "CompositeValidator",
        "module": "gpt_trader.validation.composite_validators",
        "docstring": "Combine multiple validators",
        "parameters": {
          "validators": {
            "required": true,
            "type": "<class 'gpt_trader.validation.base_validators.Validator'>"
          }
        },
        "validate_params": [
          "value",
          "field_name"
        ],
        "description": "Chains multiple validators together",
        "usage": "CompositeValidator([TypeValidator(int), RangeValidator(0, 100)])"
      }
    }
  }
}
