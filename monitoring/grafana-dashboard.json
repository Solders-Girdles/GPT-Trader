{
  "dashboard": {
    "id": null,
    "title": "GPT-Trader Production Dashboard",
    "tags": ["trading", "gpt-trader", "production"],
    "style": "dark",
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "System Health Overview",
        "type": "stat",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "up{job=\"gpt-trader\"}",
            "legendFormat": "{{instance}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "red", "value": 0}
              ]
            },
            "mappings": [
              {"options": {"0": {"text": "DOWN", "color": "red"}, "1": {"text": "UP", "color": "green"}}, "type": "value"}
            ]
          }
        }
      },
      {
        "id": 2,
        "title": "Order Execution Latency (P95)",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(order_execution_duration_seconds_bucket[5m]))",
            "legendFormat": "P95 Latency"
          }
        ],
        "yAxes": [
          {
            "label": "Latency (seconds)",
            "min": 0
          }
        ]
      },
      {
        "id": 3,
        "title": "Risk Management Overview",
        "type": "row",
        "gridPos": {"h": 1, "w": 24, "x": 0, "y": 8}
      },
      {
        "id": 4,
        "title": "Circuit Breaker Status",
        "type": "stat",
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 9},
        "targets": [
          {
            "expr": "increase(circuit_breaker_triggers_total[5m])",
            "legendFormat": "Triggers"
          }
        ]
      },
      {
        "id": 5,
        "title": "Risk Limit Breaches",
        "type": "stat",
        "gridPos": {"h": 8, "w": 6, "x": 6, "y": 9},
        "targets": [
          {
            "expr": "increase(risk_limit_breaches_total[5m])",
            "legendFormat": "Breaches"
          }
        ]
      },
      {
        "id": 6,
        "title": "Daily P&L",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 9},
        "targets": [
          {
            "expr": "daily_pnl",
            "legendFormat": "Daily P&L"
          }
        ]
      },
      {
        "id": 7,
        "title": "Order Flow Metrics",
        "type": "row",
        "gridPos": {"h": 1, "w": 24, "x": 0, "y": 17}
      },
      {
        "id": 8,
        "title": "Orders Submitted (Rate)",
        "type": "graph",
        "gridPos": {"h": 8, "w": 8, "x": 0, "y": 18},
        "targets": [
          {
            "expr": "rate(orders_submitted_total[5m])",
            "legendFormat": "Orders/sec"
          }
        ]
      },
      {
        "id": 9,
        "title": "Order Success Rate",
        "type": "stat",
        "gridPos": {"h": 8, "w": 8, "x": 8, "y": 18},
        "targets": [
          {
            "expr": "rate(orders_successful_total[5m]) / rate(orders_submitted_total[5m]) * 100",
            "legendFormat": "Success Rate %"
          }
        ]
      },
      {
        "id": 10,
        "title": "Order Failure Rate",
        "type": "stat",
        "gridPos": {"h": 8, "w": 8, "x": 16, "y": 18},
        "targets": [
          {
            "expr": "rate(orders_failed_total[5m]) / rate(orders_submitted_total[5m]) * 100",
            "legendFormat": "Failure Rate %"
          }
        ]
      },
      {
        "id": 11,
        "title": "System Resources",
        "type": "row",
        "gridPos": {"h": 1, "w": 24, "x": 0, "y": 26}
      },
      {
        "id": 12,
        "title": "CPU Usage",
        "type": "graph",
        "gridPos": {"h": 8, "w": 8, "x": 0, "y": 27},
        "targets": [
          {
            "expr": "rate(process_cpu_seconds_total[5m]) * 100",
            "legendFormat": "CPU %"
          }
        ]
      },
      {
        "id": 13,
        "title": "Memory Usage",
        "type": "graph",
        "gridPos": {"h": 8, "w": 8, "x": 8, "y": 27},
        "targets": [
          {
            "expr": "process_resident_memory_bytes / (1024*1024)",
            "legendFormat": "Memory MB"
          }
        ]
      },
      {
        "id": 14,
        "title": "Database Connections",
        "type": "graph",
        "gridPos": {"h": 8, "w": 8, "x": 16, "y": 27},
        "targets": [
          {
            "expr": "db_connections_active",
            "legendFormat": "Active Connections"
          }
        ]
      },
      {
        "id": 15,
        "title": "Integration Test Results",
        "type": "row",
        "gridPos": {"h": 1, "w": 24, "x": 0, "y": 35}
      },
      {
        "id": 16,
        "title": "Test Pass Rate",
        "type": "stat",
        "gridPos": {"h": 8, "w": 8, "x": 0, "y": 36},
        "targets": [
          {
            "expr": "integration_tests_passed_total / (integration_tests_passed_total + integration_tests_failed_total) * 100",
            "legendFormat": "Pass Rate %"
          }
        ]
      },
      {
        "id": 17,
        "title": "Recent Test Failures",
        "type": "table",
        "gridPos": {"h": 8, "w": 16, "x": 8, "y": 36},
        "targets": [
          {
            "expr": "integration_test_failure_info",
            "format": "table"
          }
        ]
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "5s"
  }
}