{
  "title": "Resource Utilization",
  "tags": [
    "trading-bot",
    "resources",
    "system"
  ],
  "timezone": "browser",
  "schemaVersion": 16,
  "version": 1,
  "refresh": "10s",
  "panels": [
    {
      "id": 1,
      "title": "Current Memory Usage",
      "type": "stat",
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 0,
        "y": 0
      },
      "targets": [
        {
          "expr": "bot_memory_used_bytes{profile=\"production\"} / 1024 / 1024",
          "legendFormat": "Memory MB",
          "refId": "A"
        }
      ],
      "options": {
        "reduceOptions": {
          "values": false,
          "calcs": [
            "lastNotNull"
          ]
        },
        "orientation": "auto",
        "textMode": "auto",
        "colorMode": "value",
        "graphMode": "area"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "decmbytes",
          "decimals": 2,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": 0,
                "color": "green"
              },
              {
                "value": 500,
                "color": "yellow"
              },
              {
                "value": 1000,
                "color": "red"
              }
            ]
          }
        }
      }
    },
    {
      "id": 2,
      "title": "Current CPU Usage",
      "type": "stat",
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 6,
        "y": 0
      },
      "targets": [
        {
          "expr": "bot_cpu_percent{profile=\"production\"}",
          "legendFormat": "CPU %",
          "refId": "A"
        }
      ],
      "options": {
        "reduceOptions": {
          "values": false,
          "calcs": [
            "lastNotNull"
          ]
        },
        "orientation": "auto",
        "textMode": "auto",
        "colorMode": "value",
        "graphMode": "area"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "decimals": 1,
          "max": 100,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": 0,
                "color": "green"
              },
              {
                "value": 50,
                "color": "yellow"
              },
              {
                "value": 80,
                "color": "red"
              }
            ]
          }
        }
      }
    },
    {
      "id": 3,
      "title": "Memory Growth Rate",
      "type": "stat",
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 12,
        "y": 0
      },
      "targets": [
        {
          "expr": "rate(bot_memory_used_bytes{profile=\"production\"}[5m]) / 1024 / 1024 * 3600",
          "legendFormat": "MB/hour",
          "refId": "A"
        }
      ],
      "options": {
        "reduceOptions": {
          "values": false,
          "calcs": [
            "lastNotNull"
          ]
        },
        "orientation": "auto",
        "textMode": "auto",
        "colorMode": "value",
        "graphMode": "none"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "decmbytes",
          "decimals": 2,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": -10,
                "color": "green"
              },
              {
                "value": 10,
                "color": "yellow"
              },
              {
                "value": 50,
                "color": "red"
              }
            ]
          }
        }
      }
    },
    {
      "id": 4,
      "title": "Average CPU (5m)",
      "type": "stat",
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 18,
        "y": 0
      },
      "targets": [
        {
          "expr": "avg_over_time(bot_cpu_percent{profile=\"production\"}[5m])",
          "legendFormat": "Avg CPU %",
          "refId": "A"
        }
      ],
      "options": {
        "reduceOptions": {
          "values": false,
          "calcs": [
            "lastNotNull"
          ]
        },
        "orientation": "auto",
        "textMode": "auto",
        "colorMode": "value",
        "graphMode": "none"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "decimals": 1,
          "max": 100,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": 0,
                "color": "green"
              },
              {
                "value": 40,
                "color": "yellow"
              },
              {
                "value": 70,
                "color": "red"
              }
            ]
          }
        }
      }
    },
    {
      "id": 5,
      "title": "Memory Usage Over Time",
      "type": "graph",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 4
      },
      "targets": [
        {
          "expr": "bot_memory_used_bytes{profile=\"production\"} / 1024 / 1024",
          "legendFormat": "Memory (MB)",
          "refId": "A"
        }
      ],
      "yaxes": [
        {
          "format": "decmbytes",
          "label": "Memory"
        },
        {
          "format": "short"
        }
      ],
      "xaxis": {
        "mode": "time"
      },
      "lines": true,
      "fill": 2,
      "linewidth": 2,
      "nullPointMode": "null",
      "tooltip": {
        "shared": true,
        "sort": 0,
        "value_type": "individual"
      },
      "legend": {
        "show": true,
        "values": true,
        "min": true,
        "max": true,
        "current": true,
        "avg": true
      }
    },
    {
      "id": 6,
      "title": "CPU Usage Over Time",
      "type": "graph",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 4
      },
      "targets": [
        {
          "expr": "bot_cpu_percent{profile=\"production\"}",
          "legendFormat": "CPU %",
          "refId": "A"
        }
      ],
      "yaxes": [
        {
          "format": "percent",
          "label": "CPU",
          "max": 100,
          "min": 0
        },
        {
          "format": "short"
        }
      ],
      "xaxis": {
        "mode": "time"
      },
      "lines": true,
      "fill": 2,
      "linewidth": 2,
      "nullPointMode": "null",
      "tooltip": {
        "shared": true,
        "sort": 0,
        "value_type": "individual"
      },
      "legend": {
        "show": true,
        "values": true,
        "min": true,
        "max": true,
        "current": true,
        "avg": true
      }
    },
    {
      "id": 7,
      "title": "Memory & CPU Combined",
      "type": "graph",
      "gridPos": {
        "h": 8,
        "w": 24,
        "x": 0,
        "y": 12
      },
      "targets": [
        {
          "expr": "bot_memory_used_bytes{profile=\"production\"} / 1024 / 1024",
          "legendFormat": "Memory (MB)",
          "refId": "A"
        },
        {
          "expr": "bot_cpu_percent{profile=\"production\"} * 10",
          "legendFormat": "CPU % (x10)",
          "refId": "B"
        }
      ],
      "yaxes": [
        {
          "format": "short",
          "label": "Memory MB / CPU% x10"
        },
        {
          "format": "short"
        }
      ],
      "xaxis": {
        "mode": "time"
      },
      "lines": true,
      "fill": 1,
      "linewidth": 2,
      "nullPointMode": "null",
      "tooltip": {
        "shared": true,
        "sort": 0,
        "value_type": "individual"
      },
      "legend": {
        "show": true,
        "values": true,
        "min": false,
        "max": false,
        "current": true,
        "avg": true
      }
    }
  ],
  "uid": "resource-utilization"
}
